<div class="container text-center">
<h2 class="text-white">吃飯紀錄</h2>
<table class="table">
  <tbody class="text-white">
    <% dashboards.order(finished_at: :desc).meal.each do |dashboard| %>
      <tr>
          <td><%= dashboard.title %></td>
          <td><%= dashboard.finished_at.to_date %></td>
          <td><%= dashboard.content   %></td>
          <td><%= link_to '詳情', dashboard_child_eat_path(child_id: params[:id],id: dashboard.id) %></td>

      </tr>
    <% end %>
  </tbody>
</table>

</div>