<%= form_with scope: :pick_up, url: dashboard_child_pick_up_index_path do |form| %>
  <%= form.label :name, "姓名" %>
  <%= form.text_field :name %>

  <%= form.label :pick_up_pic, "照片" %>
  <%= form.file_field :pick_up_pic %>

  <%= form.label :relationship, "與小朋友的關係" %>
  <%= form.text_field :relationship %>

  <%= form.label :note, "備註" %>
  <%= form.text_area :note %>

  <%= form.submit "送出" %>
<% end %>