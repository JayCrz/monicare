<div class="container text-center">
<h2 class="text-white">吃飯紀錄</h2>
<table class="table">
  <tbody class="text-white">
      <% dashboards.meal.each do |dashboard| %>
      <tr>
          <td><%= dashboard.title %></td>
          <td><%= dashboard.started_at %></td>
          <td><%= link_to '編輯', edit_teacher_dashboard_child_eat_path(child_id: params[:id],id: dashboard.id) %></td>
      </tr>
      <% end %>
  </tbody>
</table>
<%= link_to '新增吃飯紀錄', new_teacher_dashboard_child_eat_path(child_id: params[:id]),class:'btn btn-warning'%>
</div>