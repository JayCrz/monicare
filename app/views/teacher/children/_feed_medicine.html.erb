<div class="container text-center">
<h2 class="text-white">用藥明細</h2>
<table class="table">
  <tbody class="text-white">


    <% dashboards.medicine.each do |dashboard| %>
      <tr>
          <td><%= dashboard.title %></td>
          <td><%= dashboard.started_at.to_date %></td>
